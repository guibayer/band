*{
	margin:0;
	padding:0;
}
body{
	background:#D3D3D3;
}
table{
	border:none;
	border-collapse: collapse;
	font:normal 14px sans-serif;
}
table th{
	background:#37352D;
	height:45px;
	color:#F7F7F7;
	text-align:center;
}
table tr{
text-align:center;
}
.trcomfundo {
	background:#8B8774;
}
.trsemfundo {
	background:#B7B3AA;
}
.trcomfundoover {
	background:#B7B3AA;
}
.delet:hover, .edit:hover {
	opacity:1.0;
}
.edit {
  background: url(../images/editar.png) no-repeat 0 0;
} 
.delet {
	background: url(../images/exclui.png) no-repeat 0 0;
}
.edit, .delet{
  display: inline-block;
	width: 37px;
	text-indent: -9999px;
	overflow: hidden;
	float:right;
	opacity:0.2;
	filter:alpha(opacity=100);
	-webkit-transition: all .3s linear;
	-moz-transition: all .3s linear;
	-o-transition: all .3s linear;
	transition: all .3s linear;
}
.mass{
	width:850px;
	margin:10px auto;
}
.discografy_1{
	width:850px;
	margin:0px auto;
  margin-top:15px;
	background:#B7B3AA;
}
.discografy{
	width:850px;
	margin:0 auto;
	background:#B7B3AA;
}

